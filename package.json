{
  "name": "hondata-ai-tuning-assistant",
  "version": "1.0.0",
  "description": "A desktop application that uses local and cloud AI to analyze Hondata S300 datalogs and provide tuning advice.",
  "main": "electron.js",
  "type": "commonjs",
  "homepage": "./",
  "author": "YourName",
  "license": "MIT",
  "scripts": {
    "start": "npm run build && electron .",
    "dev": "npm run build && electron . --dev",
    "build:css": "tailwindcss -i ./styles/tailwind.css -o ./dist/tailwind.css",
    "build:js": "esbuild index.tsx --bundle --outfile=dist/bundle.js --platform=browser --format=iife --external:electron --define:process.env.NODE_ENV='\"production\"' --loader:.tsx=tsx --loader:.ts=tsx",
    "build": "npm run build:css && npm run build:js",
    "clean": "rm -rf dist/ || rmdir /s dist\\",
    "rebuild": "npm run clean && npm run build"
  },
  "dependencies": {
    "@google/genai": "^0.14.0",
    "electron": "^31.2.1",
    "electron-dl": "^3.5.0",
    "papaparse": "^5.4.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^2.12.7"
  },
  "devDependencies": {
    "@types/papaparse": "^5.3.14",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "esbuild": "^0.23.0",
    "tailwindcss": "^3.4.4"
  }
}